{
    "name": "andresvideo",
    "version": 2,
    "builds": [{"src": "index.js", "use": "@now/node"}],
    "routes": [{"src": "/(.*)", "dest": "/index.js"}],
    "env": {
        "DB_USER" : "@andresvideos-db-user",
        "DB_PASSWORD" : "@andresvideos-db-password",
        "DB_HOST" : "@andresvideos-db-host",
        "DB_NAME": "@andresvideos-db-name"
    }
}